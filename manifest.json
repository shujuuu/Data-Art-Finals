{
  "name": "Hello World",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["contentScripts.js"]
  }],
  "background": {
    "scripts": ["data.js", "background.js"],
    "persistent": false
  },
  "browser_action": {

  },
  "permissions": [
    "history",
    "webNavigation",
    "tabs"
  ]
}
