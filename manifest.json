{
    "name": "Chrome History",
    "description": "Put browse history into New York Times",
    "version": "1.0",
    "manifest_version": 2,
    "page_action": {
        // "default_popup": "popup.html",
        "default_icon": "icon.png",
        "default_title": "this is an xxxx url address"
    },
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"]
    }],
    "permissions": [
        "history"
    ]
}